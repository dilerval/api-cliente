// Criando o ambiente virtual
python3 -m venv ./venv

// Ativando o ambiente
source venv/bin/activate

// Ver módulos instalados
pip freeze

// Instalando módulos listados no arquivos
pip install -r requirements.txt

// COmo o cursos era antigo as dependencias n instalaram com o comando anterior
// tive que instalar pacote por pacote, pegando o requirements.txt
python3 -m pip install --upgrade PACKAGE_NAME

// Gerando novo arquivo
pip freeze > requirements.txt


//Iniciando servidor
 python3 manage.py runserver

//Gerando migração
python3 manage.py makemigrations

// Rodando as migrações
python3 manage.py migrate

// Criando um super usuário
python3 manage.py createsuperuser

http://localhost:8000/admin/


// Acessando o shell para e representação da classe ClienteSerializer
python3 manage.py shell

>>>from clientes.serializers import ClienteSerializer
>>>s = ClienteSerializer()
>>>print(repr(s))


#  Biblioteca de validação de documentos brasilieros
validate_docbr
